<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mt.orderweed">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".signup_login_tabs" />
        <activity android:name=".forgot_password_email" />
        <activity android:name=".Reset_password" />
        <activity android:name=".product_details" />
        <activity android:name=".write_review" />
        <activity
            android:name=".Home"
            android:label="@string/title_activity_home">
            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
        </activity>
        <activity android:name=".Home_Products"></activity>
        <activity
            android:name=".Favorites"
            android:label="@string/title_activity_favorites"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>